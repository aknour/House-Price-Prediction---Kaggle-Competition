import pandas as pd 
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns 

train_df = pd.read_csv('train.csv')

Summmary and Statistics of Training DF

#Understand the structure of the DS
train_df.head()

#Statistical summary of the DS
train_df.describe()

#Check for missing or NULL values 
train_df.info()
train_df.isnull().sum()

Filling Missing Values

lotfrontage_median = train_df['LotFrontage'].median()
train_df['LotFrontage'].fillna(lotfrontage_median, inplace=True)

train_df['LotFrontage'].fillna(train_df['LotFrontage'].mean(), inplace=True)
print(train_df['LotFrontage'].isnull().sum())

# Fill missing values for other columns (demonstration purposes)
for column in train_df.columns:
    if train_df[column].dtype == "object":  # for string columns
        train_df[column] = train_df[column].fillna(train_df[column].mode()[0])
    else:  # for numeric columns
        train_df[column] = train_df[column].fillna(train_df[column].median())


